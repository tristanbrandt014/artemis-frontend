    "request-promise": "^4.2.2",
    "source-map-explorer": "^1.5.0",
    "style-loader": "0.18.2",
    "styled-components": "^2.2.1",
    "styled-normalize": "^2.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
